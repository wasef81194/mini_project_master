<form method="post" action="{{ path('app_fds_delete', {'id': fds.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer ce FDS?');" class="mt-3">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ fds.id) }}">
    <button type="submit" class="btn btn-danger" onclick="return confirm('Voulez-vous vraiment supprimer ce FDS?');">
        <i class="fas fa-exclamation-triangle"></i> Supprimer ce fichier FDS
    </button>
</form>
